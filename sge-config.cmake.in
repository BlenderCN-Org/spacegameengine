include(
	FcpptExport
)

set(
	SGE_BINARY_DIR
	@CMAKE_BINARY_DIR@
)

set(
	SGE_INSTALL_LIBRARY_DIR
	@INSTALL_LIBRARY_DIR@
)

set(
	FCPPT_EXPORT_IN_SOURCE_BUILD
	@FCPPT_UTILS_IN_SOURCE_BUILD@
)

fcppt_export_init(
	"sge"
	"${SGE_BINARY_DIR}"
	"${SGE_INSTALL_LIBRARY_DIR}"
)

fcppt_export_generate_targets(
	"sgecore;@SGE_LIBRARIES@"
	"${sge_USE_STATIC_LIBS}"
)

foreach(
	COMPONENT
	${sge_FIND_COMPONENTS}
)
	set(
		CURRENT_TARGET
		"sge${COMPONENT}_TARGET"
	)

	if(
		TARGET
		"${${CURRENT_TARGET}}"
	)
		list(
			APPEND
			sge_TARGETS
			"${${CURRENT_TARGET}}"
		)
	else()
		message(
			"${${CURRENT_TARGET}}"
		)
		message(
			"sge.${COMPONENT} has not been built!"
		)

		set(
			sge_FOUND
			FALSE
		)
	endif()
endforeach()

unset(
	CURRENT_TARGET
)

if(
	sge_USE_STATIC_LIBS
)
	set(
		sge_DEFINITIONS
		"-D SGE_STATIC_LINK"
	)
endif()

set(
	sge_INCLUDE_DIRS
	"@Boost_INCLUDE_DIRS@;@awl_INCLUDE_DIRS@;@mizuiro_INCLUDE_DIRS@;@sge_INCLUDE_DIR@"
)
